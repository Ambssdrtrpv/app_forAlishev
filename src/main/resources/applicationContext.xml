<?xml version="1.0" encoding="UTF-8"?>
<beans  xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:context="http://www.springframework.org/schema/context"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd">
  <context:property-placeholder location="classpath:musicPlayer.properties"/>
    <bean id="musicBean1" class="ru.myApp.forAlishev.ClasicalMusic"></bean>
    <bean id="musicBean2" class="ru.myApp.forAlishev.RapMusic"></bean>
    <bean id="musicBean3" class="ru.myApp.forAlishev.RockMusic">
    </bean>
    <bean id="musicPlayer" class="ru.myApp.forAlishev.MusicPlayer">
        <property name="musicList">
            <list>
                <ref bean="musicBean1"></ref>
                <ref bean="musicBean2"></ref>
                <ref bean="musicBean3"></ref>
            </list>
        </property>
    </bean>

</beans>